{
  "name": "linky/tracker-package",
  "description": "Linky tracking package for Laravel",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/http": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "guzzlehttp/guzzle": "^7.8"
  },
  "autoload": {
    "psr-4": {
      "Linky\\Tracker\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Linky\\Tracker\\LinkyServiceProvider"
      ],
      "aliases": {
        "Linky": "Linky\\Tracker\\Facades\\Linky"
      }
    }
  }
}