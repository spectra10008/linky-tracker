{
  "name": "linky/tracker-package",
  "description": "Linky tracking package for Laravel",
  "type": "library",
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^12.0",
    "illuminate/http": "^12.0",
    "guzzlehttp/guzzle": "^7.8"
  },
  "autoload": {
    "psr-4": {
      "Linky\\Tracker\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Linky\\Tracker\\LinkyServiceProvider"
      ],
      "aliases": {
        "Linky": "Linky\\Tracker\\Facades\\Linky"
      }
    }
  }
}